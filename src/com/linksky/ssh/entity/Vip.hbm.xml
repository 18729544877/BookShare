<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.linksky.ssh.entity">
	<class name="Vip" table="sys_vip">
		<id name="vipId" column="vip_id">
			<generator class="identity" />
		</id>
		<property name="vipAccount" column="vip_account" />
		<property name="vipPassword" column="vip_password"></property>
		<property name="vipNumber" column="vip_number"></property>
		<property name="vipName" column="vip_name"></property>
		<property name="vipSex" column="vip_sex"></property>
		<property name="vipBirthday"  type="java.sql.Date" column="vip_birthday"></property>
		<property name="vipPhone" column="vip_phone"></property>
		<property name="vipIdcard" column="vip_idcard"></property>
		<property name="vipLevel" column="vip_level"></property>
		<property name="vipJoinDate" column="vip_join_date"></property>
	
	</class>
	
</hibernate-mapping>

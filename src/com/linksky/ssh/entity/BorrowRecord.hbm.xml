<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.linksky.ssh.entity">
	<class name="BorrowRecord" table="borrow_record">
		<id name="borrowId" column="borrow_id">
			<generator class="identity" />
		</id>
		<property name="vipId" column="vip_id" />
		<property name="bookId" column="book_id"></property>
		<property name="borrowDate" column="borrow_date"></property>
		<property name="rebackDate" column="reback_date"></property>
		<property name="borrowState" column="borrow_state"></property>
		<property name="note" column="note"></property>
	
	</class>
	
</hibernate-mapping>
